{
  "name": "preview-in-browser",
  "description": "Properly view your design in browser",
  "version": "1.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Preview in browser",
    "manifest": "src/manifest.json",
    "main": "preview-in-browser.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "gaddafirusli <gaddafirusli@gmail.com>",
  "homepage": "https://github.com/gaddafirusli/Preview-in-browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaddafirusli/Preview-in-browser"
  }
}
